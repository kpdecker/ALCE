{
  "name": "alce",
  "version": "1.0.0",
  "description": "Accepting Language Config Environment",
  "keywords": [
    "json parser whitespace comments configuration"
  ],
  "dependencies": {
    "esprima": "~1.0.4",
    "estraverse": "~1.3.0"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-jshint": "~0.6.2",
    "grunt-mocha-cov": "0.0.7",
    "mocha": "~1.10.0",
    "should": "~1.2.2"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "main": "lib/alce.js",
  "scripts": {
    "blanket": {
      "pattern": "//^((?!/node_modules/)(?!/test/).)*$/",
      "data-cover-flags": {
        "branchTracking": true
      }
    },
    "test": "./node_modules/.bin/mocha"
  }
}
